public class Main {
	static int swap(int a, int b, int c) {
		c = a;
		a = b;
		b = c;
		c = 0;
		System.out.println("value after swap of a is: " +  a + " & b is : " + b + " & c is: " + c);
		return 0;
	}
	public static void main(String[] args) {
		int a = 5, b = 7, c = 0;
		System.out.println("value before swap of a is: " +  a + " & b is : " + b);
		swap(a, b, c);
	}
}
